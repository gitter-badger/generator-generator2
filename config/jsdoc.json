{
	"opts": {
		"destination": "./build/jsdoc",
		"encoding": "utf8",
		"recurse": true,
		"tutorials": "./docs/tutorials"
	},
	"tags": {
		"allowUnknownTags": false
	},
	"plugins": [
		"plugins/markdown",
		"config/jsdoc_plugin"
	],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	}
}
