<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="diary.fx.app.views.realizations.RealizationsPresenter">
   <children>
      <TableView fx:id="table" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="date" minWidth="40.0" prefWidth="100.0" text="Date" />
          <TableColumn fx:id="importance" minWidth="40.0" prefWidth="100.0" text="Importance" />
            <TableColumn fx:id="bookmarked" minWidth="40.0" prefWidth="100.0" text="Bookmarked at" />
            <TableColumn fx:id="tags" prefWidth="75.0" text="Tags" />
            <TableColumn fx:id="body" minWidth="40.0" prefWidth="100.0" text="Body" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
